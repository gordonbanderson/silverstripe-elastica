{
	"name": "weboftalent/elastica",
	"description": "Provides Elastic Search integration for SilverStripe DataObjects using Elastica.  Fork of module by AJ Short",
	"type": "silverstripe-module",
	"homepage": "https://github.com/gordonbanderson/silverstripe-elastica",
	"keywords": ["silverstripe", "search", "elasticsearch", "elastica"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Andrew Short",
			"email": "andrewjshort@gmail.com"
		},
		{
			"name": "Marcus Nyeholt",
			"email": "marcus@silverstripe.com.au"
		},
		{
			"name": "Gordon Anderson",
			"email": "gordon.b.anderson@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/gordonbanderson/silverstripe-elastica/issues"
	},
	"require": {
		"ruflin/elastica": "~3.0",
		"silverstripe/cms": "~3.1",
		"silverstripe/tagfield": "~1.2",
		"briceburg/silverstripe-pickerfield" : "dev-master"
	},
	"autoload": {
		"psr-0": { "SilverStripe\\Elastica": "src/" }
	},
	"extra": {
		"installer-name": "elastica"
	},
	"replace": {
		"ajshort/silverstripe-elastica": "self.version"
	}
}
